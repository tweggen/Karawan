{
  "resources": {
    "list": [
      { "uri": "shaders/LIghtingVS.vert" },
      { "uri": "shaders/LIghtingFS.frag" },
      { "uri": "shaders/ScreenFS.frag" },
      { "uri": "griddata.json" }
    ]
  },
  
  "globalSettings": {
    "list": [
      { "key": "nogame.framebuffer.resolution", "value": "1280x720" }
    ]
  },

  "defaults": {
    "loader": {
      "assembly": "grid.dll"
    }
  },

  "implementations": {},
  
  "modules": {
    "root": {
      "className": "grid.Main"
    }
  },

  "scenes": {
    "catalogue": {
      "grid": {
        "className": "grid.Scene"
      }
    },
    "startup": "grid"
  },

  "textures": {
    "channels": {}
  }
}
